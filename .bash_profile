# some more ls aliases
alias ll='ls -alF'
alias la='ls -Ap'
alias l='ls -CF'
alias c='clear'
alias lp='ls -p'

#Screen aliases
alias sl="screen -ls"
alias sr="screen -r"

# Rails Specific aliases
alias rs='screen -S rails_server rails s'
alias gu='screen -S guard_screen guard'

# General Program aliases
alias irssi='screen -S irssi_screen irssi'
